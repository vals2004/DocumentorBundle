{
    "name": "vals2004/documentor-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle to implement phpDocumentor into any project from which the documentation is accessible with a browser.",
    "keywords": ["phpdocumentor"],
    "homepage": "https://github.com/vals2004/DocumentorBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Artur Gajewski",
            "email": "info@arturgajewski.com"
        },
        {
            "name": "Tim Hovius",
            "email": "timhovius@gmail.com"
        },
        {
            "name": "Valeriy Zavolodko",
            "email": "vals2004@gmail.com"
        }
    ],

    "require": {
        "php": ">=5.3.3",
        "phpdocumentor/phpdocumentor": "*"
    },

    "autoload": {
        "psr-0": { "Documentor\\Bundle\\DocumentorBundle": "" }
    },

    "target-dir": "Documentor/Bundle/DocumentorBundle"

}
